{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=your_server_name;Database=your_store_database;User Id=your_db_user;Password=your_db_password;",
    "IdentityConnection": "Server=your_server_name;Database=your_identity_database;User Id=your_db_user;Password=your_db_password;",
    "RedisConnection": "your_redis_host:6379"
  },
  "BaseUrl": "https://your-domain.com",
  "JwtOptions": {
    "Issuer": "https://your-issuer-domain.com",
    "Audience": "your-audience-name",
    "DurationInDays": 5,
    "SecretKey": "your_jwt_secret_key_min_32_chars"
  },
  "Stripe": {
    "Secretkey": "sk_test_your_stripe_secret_key"
  },
  "AllowedHosts": "*"
}
